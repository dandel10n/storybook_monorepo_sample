  },
  "devDependencies": {
    "@storybook/addon-essentials": "6.0.28",
    "@storybook/addon-links": "6.0.28",
    "@storybook/vue": "6.0.28",
    "node-sass-magic-importer": "5.3.2",
    "path": "0.12.7",
    "vue-cli-plugin-storybook": "2.0.0"
  }
}
